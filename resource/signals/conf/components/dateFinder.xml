<?xml version="1.0" encoding="UTF-8"?>

<component xmlns="http://www.attivio.com/configuration/type/componentType" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="dateFinder" class="com.attivio.platform.transformer.ingest.field.ExtractRegexEntities" xsi:schemaLocation="http://www.attivio.com/configuration/type/componentType classpath:/xsd/type/componentType.xsd">
  <properties>
    <property name="entityType" value="date"/>
    <property name="outputAsField" value="false"/>
    <property name="outputAsEntityTag" value="false"/>
    <list name="regularExpressions">
      <entry value="([0123]?\d/[0123]?\d/[12]\d{1,3})"/>
      <entry value="(?i)((January|February|March|April|May|June|July|August|September|October|November|December|Jan\.?|Feb\.?|Mar\.?|Apr\.?|Jun\.?|Jul\.?|Aug\.?|Sep\.?|Sept\.?|Oct\.?|Nov\.?|Dec\.?) \d{1,2}(st|nd|rd|th)?,? [12]\d{3})"/>
      <entry value="(?i)(\d{1,2}?(st|nd|rd|th)? (January|February|March|April|May|June|July|August|September|October|November|December|Jan\.?|Feb\.?|Mar\.?|Apr\.?|Jun\.?|Jul\.?|Aug\.?|Sep\.?|Sept\.?|Oct\.?|Nov\.?|Dec\.?) [12]\d{3})"/>
      <entry value="[Ii]n ([12]\d{3})"/>
    </list>
  </properties>
</component>
